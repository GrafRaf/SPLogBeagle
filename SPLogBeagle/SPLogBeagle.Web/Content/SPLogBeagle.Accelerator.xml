<?xml version="1.0" encoding="utf-8"?>
<openServiceDescription xmlns="http://www.microsoft.com/schemas/openservicedescription/1.0">
  <homepageUrl>https://github.com/GrafRaf/SPLogBeagle</homepageUrl>
  <display>
    <name>SPLogBeagle</name>
  </display>
  <activity category="Find">
    <activityAction context="selection">
      <execute action="http://www.example.com/Home/Index">
        <parameter name="doc" value="{documentUrl}" />
        <parameter name="title" value="{documentTitle?}" />
        <parameter name="term" value="{selection}" />
      </execute>
    </activityAction>
  </activity>
</openServiceDescription>